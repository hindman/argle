# GRAMMAR
Grammar(
    OptHelp(
        text = 'Python regular expression'
        Positional(
            sym = None
            dest = 'rgx'
            symlit = False
            choices = ()
            quantifier = None
    OptHelp(
        text = 'Path(s) to input'
        Bracketed(
            quantifier = None
            Positional(
                sym = None
                dest = 'path'
                symlit = False
                choices = ()
                quantifier = Quantifier(m=1, n=None, greedy=True)
    OptHelp(
        text = 'Ignore case'
        Bracketed(
            quantifier = None
            Option(
                dest = 'i'
                quantifier = None
            Option(
                dest = 'ignore-case'
                quantifier = None
    OptHelp(
        text = 'Select non-matching lines'
        Bracketed(
            quantifier = None
            Option(
                dest = 'v'
                quantifier = None
            Option(
                dest = 'invert-match'
                quantifier = None
