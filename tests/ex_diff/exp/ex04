
# SPEC

pgrep
[-i] [-v]
    <rgx> <path>
[--foo] <blort>


# PARSER_DEBUG

parse(mode_check = 'started')
    eat(wanted = 'section_name')
        get_next_token(lexed = 'name')
    eat(wanted = 'name')
        get_next_token(lexed = 'name')
        taste(isfirst = True)
        get_next_token(returned = 'name')
        eat(eaten = 'name', text = 'pgrep', pos = 1, line = 2, col = 1)
parse(mode = 'variant')
